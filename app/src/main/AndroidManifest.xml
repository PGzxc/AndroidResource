<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidresource">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.raw.RawMusicActivity" />
        <activity android:name=".ui.raw.RawPicActivity"/>
        <activity android:name=".ui.asset.AssetWebViewActivity" android:label="@string/app_name"/>
        <activity android:name=".ui.asset.AssetImgActivity" android:label="@string/asset_web"/>
        <activity android:name=".ui.asset.AssetMusicActivity" android:label="@string/asset_img"/>
    </application>

</manifest>